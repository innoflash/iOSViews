<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Toolbar">
        <attr name="toolbar_title" format="string" />
        <attr name="toolbar_icon" format="reference" />
        <attr name="toolbar_has_options" format="boolean" />
        <attr name="toolbar_no_back_button" format="boolean" />
        <attr name="toolbar_background" format="color" />
        <attr name="toolbar_title_color" format="color" />
        <attr name="toolbar_back_color" format="color" />
    </declare-styleable>
    <declare-styleable name="ChipView">
        <attr name="ios_chip_icon" format="reference" />
        <attr name="ios_chip_text" format="string" />
    </declare-styleable>
    <declare-styleable name="ChipLineView">
        <attr name="ios_chipline_key" format="string" />
        <attr name="ios_chipline_value" format="string" />
        <attr name="ios_chipline_icon" format="reference" />
    </declare-styleable>
    <declare-styleable name="DialogToolbar">
        <attr name="ios_dialog_title" format="string" />
    </declare-styleable>
    <declare-styleable name="ImageButton">
        <attr name="image_button_image" format="reference" />
        <attr name="image_button_text" format="string" />
    </declare-styleable>
    <declare-styleable name="InputField">
        <attr name="input_heading" format="string" />
        <attr name="input_hint" format="string" />
        <attr name="input_value" format="string" />
        <attr name="input_heading_color" format="color" />
        <attr name="input_icon" format="reference" />
        <attr name="input_enabled" format="boolean" />
    </declare-styleable>
    <declare-styleable name="SpinnerView">
        <attr name="spinner_heading" format="string" />
        <attr name="spinner_selection" format="integer" />
        <attr name="spinner_heading_color" format="color" />
        <attr name="spinner_icon" format="reference" />
    </declare-styleable>
    <declare-styleable name="ListItemView">
        <attr name="liv_icon" format="reference" />
        <attr name="liv_icon_padding" format="dimension" />
        <attr name="liv_text" format="string" />
        <attr name="liv_arrow" format="reference" />
        <attr name="liv_has_line" format="boolean" />
        <attr name="liv_has_image" format="boolean" />
        <attr name="liv_circular_image" format="boolean" />
        <attr name="liv_text_color" format="color" />
    </declare-styleable>
    <declare-styleable name="ListItemView2">
        <attr name="liv2_icon" format="reference" />
        <attr name="liv2_icon_padding" format="dimension" />
        <attr name="liv2_head_text" format="string" />
        <attr name="liv2_content_text" format="string" />
        <attr name="liv2_has_line" format="boolean" />
        <attr name="liv2_has_image" format="boolean" />
        <attr name="liv2_circular_image" format="boolean" />
        <attr name="liv2_header_color" format="color" />
        <attr name="liv2_content_color" format="color" />
    </declare-styleable>
    <declare-styleable name="ListItemView3">
        <attr name="item_heading" format="string" />
        <attr name="item_content" format="string" />
        <attr name="item_footer" format="string" />
        <attr name="item_badge" format="string" />
        <attr name="item_icon" format="reference" />
        <attr name="item_icon_padding" format="dimension" />
        <attr name="item_has_type" format="boolean" />
        <attr name="item_circular_image" format="boolean" />
        <attr name="item_heading_color" format="color" />
        <attr name="item_content_color" format="color" />
        <attr name="item_type" format="enum">
            <enum name="none" value="0" />
            <enum name="green" value="1" />
            <enum name="blue" value="2" />
            <enum name="red" value="2" />
            <enum name="orange" value="2" />
        </attr>
    </declare-styleable>
    <declare-styleable name="NoHeaderInputField">
        <attr name="field_icon" format="reference" />
        <attr name="field_hint" format="string" />
        <attr name="field_value" format="string" />
        <attr name="field_activated" format="boolean" />
    </declare-styleable>
</resources>